tion scheduleCallback(callback) {\n  const startTime = getTime()\n\n  const newTask = {\n    callback,\n    startTime,\n    dueTime: startTime + 300\n  }\n\n  push(taskQueue, newTask)\n  // currentCallback = flush\n  // planWork()\n}\n\n\n// show on window\nwindow.Maybe = Maybe\nwindow.Either = Either\n"},"lineCount":null}},"error":null,"hash":"5f609db1cfd203d55bab1ff2446e2393","cacheData":{"env":{}}}